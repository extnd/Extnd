<action title="Process Selected Docs Example"><code for="web" event="onClick"><javascript>// 'this' returns a reference to the underlying grid/uiview
alert("This example will alert the UNIDs of each selected document4");

alert("first, we get the selections the 'Ext way' using this.getUIView().getSelectionModel().getSelections()");
var selections = this.getUIView().getSelectionModel().getSelections();
for (var i=0; i&lt;selections.length; i++) {
	alert(selections[i].unid);
}

alert("next, we get the selections using this.getUIView().getDocuments() 'method'");
var selections2 = this.getUIView().getDocuments();
for (var i=0; i&lt;selections2.length; i++) {
	alert(selections2[i].unid);
}

alert("finally, we do it the 'Notes like way' using this.getUIView().documents 'property'");
var selections3 = this.getUIView().documents;
for (var i=0; i&lt;selections3.length; i++) {
	alert(selections3[i].unid);
}</javascript></code></action>
